<script>

    function getnews() {
    
        fetch('http://newsapi.org/v2/top-headlines?country=us&category=general&apikey=e34c767bd0b34078a7a790f5c0dd9ba5').then((res)=>{
            return res.json()
        }).then((data)=>{
            console.log(data)
            data.articles.forEach(article =>{
                let h2 = document.createElement('h2');
                let a = document.createElement('a');
                let description = document.createElement('p');
                a.setAttribute('href',article.url);
                a.textContent = article.title;
                h2.appendChild(a);
                description.textContent=article.description;
                document.getElementById("newsList").appendChild(h2);
                document.getElementById("newsList").appendChild(description);
            })
        })
    }
</script>
<body>
    
    <div class="col-md-4 col-md-offset-4 col-mb-2 "style="margin-bottom: 60px;">
        <nav class="navbar navbar-default">
            <h1><a class="navbar-register" href="./index"><span>N</span>ewsRoom</a></h1>
        </nav>
    </div>
    <div class= "col-sm-12" style="margin-left: 20%;">
        <h1>General</h1>
        <hr style="margin-right: 40%;">
    </div>
    <div class="col-md-4 col-md-offset-4 col-mb-2 " id="newsList"><ul><p id="descri"></p></ul></div>
    <button onclick="getnews()">Try it </button>

</body>