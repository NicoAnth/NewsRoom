<!-- contact -->
<div id="mail" class="contact ">
    <div class="container">
        <h3 class="wthree_head">Send mails</h3>
        <div class="w3layouts_mail_grids">
            <!-- afin de centrer la div on a (12-7)/2 = 3 qui est la moitiÃ© en taille restante -->
            <div class="w3layouts_mail_grid_right ">
                <form action="./newsletter/sendMails" method="post">
                    <div class="col-md-6 wthree_contact_left_grid ">
                        <input type="email" name="to" placeholder="to" required="">
                    </div>
                    <div class="col-md-6 wthree_contact_left_grid ">
                        <input type="text" name="subject" placeholder="subject" required="">
                    </div>
                    <div class="clearfix" style="margin-left: 2%;">
                        <textarea name="text" placeholder="text" required=""></textarea>
                        <div style="margin-left: 1%;">
                            <input type="submit" value="send">
                            <input type="reset" value="Clear">
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</footer>
